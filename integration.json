{
    "id": "legalaidsummarybot",
    "name": "LegalAidSummaryBot",
    "description": "Summarizes text messages and provides legal assistance via /legal commands in a Telex channel.",
    "type": "modifier",
    "version": "1.0.0",
    "settings": {
      "endpoint": "https://legalaidsummarybot.vercel.app/webhook",
      "auth_token": "API_TOKEN = process.env.HUGGING_FACE_TOKEN;",
      "trigger_prefix": "/legal"
    },
    "events": [
      "message.created",
      "file.uploaded"
    ]
  }





  